!URxvt.font:xft:agave Nerd Font:pixelsize=14, xft:DejaVu Sans Mono Nerd Font:pixelsize=14, xft:Noto Color Emoji:pixelsize=14
URxvt.font:xft:Lilex:pixelsize=13:weight=medium, xft:DejaVu Sans Mono Nerd Font:pixelsize=13, xft:Noto Color Emoji:pixelsize=14
!URxvt.font:xft:Fixedsys Excelsior:pixelsize=14, xft:DejaVu Sans Mono Nerd Font:pixelsize=14
!URxvt.letterSpace: -3
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt*iso14755: false
URxvt*iso14755_52: false

URxvt*perl-ext-common: resize-font,matcher
URxvt.url-launcher: firefox
URxvt.matcher.button: Ctrl-1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

#define black    #080808
#define bblack   #464646
#define red      #626262
#define bred     #626262
#define yellow   #B2B2B2
#define byellow  #B2B2B2
#define green    #868686
#define bgreen   #009900
#define blue     #00BAFF
#define bblue    #767676
#define magenta  #E41D91
#define bmagenta #FF00AF
#define cyan     #949494
#define bcyan    #949494
#define white    #eeeeee
#define bwhite   #949494
#define fg       #464646

*.foreground:     fg
#ifdef background_opacity
*.background:     [background_opacity]white
#else
*.background:     white
#endif
*.cursorColor:    #528BFF
*.underlineColor: yellow
*.color0:         black
*.color8:         bblack
*.color1:         red
*.color9:         bred
*.color3:         yellow
*.color11:        byellow
*.color2:         green
*.color10:        bgreen
*.color4:         blue
*.color12:        bblue
*.color5:         magenta
*.color13:        bmagenta
*.color6:         cyan
*.color14:        bcyan
*.color7:         white
*.color15:        bwhite

*.color66: fg
*color66:  fg

pmenu.faceName:xft:Lilex:pixelsize=13:weight=medium, xft:DejaVu Sans Mono Nerd Font:pixelsize=13, xft:Noto Color Emoji:pixelsize=14
pmenu.background: fg
pmenu.foreground: white
pmenu.activeBackground: white
pmenu.activeForeground: fg
pmenu.borderColor: blue
